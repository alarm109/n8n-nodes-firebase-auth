		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/FirebaseAuthApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/FirebaseAuth/FirebaseAuth.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"firebase-admin": "^12.7.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
