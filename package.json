{
	"name": "n8n-nodes-firebase-admin-auth-get-uid-from-email",
	"version": "1.0.0",
	"description": "Firebase Admin Auth get the uid from the email of the user",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "Matas DziadaraviÄius",
		"email": "matas109@gmail.com",
